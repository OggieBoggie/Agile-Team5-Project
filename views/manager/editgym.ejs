<div class="container">
  <h1 class="page-header">Edit</h1>
  <form action=<%= "/gym/edit/" + gym.id %> method="POST">
    <div class="form-group">
      <label for="title">Name</label>
      <input type="text" name="name" class="form-control" id="titleInput" value="<%= gym.name %>">
    </div>

    <div class="form-group">
      <label for="title">Address</label>
      <input type="text" name="address" class="form-control" id="titleInput" value="<%= gym.address %>">
    </div>
    <button type="submit" class="btn btn-outline-secondary">Submit</button>
  </form>
  <a href="/home" class="btn btn-outline-secondary">Back</a>
</div>
